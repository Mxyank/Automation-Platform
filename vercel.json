{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist/public",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/handler"
        },
        {
            "source": "/((?!api/|assets/|favicon.ico|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}